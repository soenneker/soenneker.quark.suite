<?xml version="1.0" encoding="utf-8"?>

<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <Nullable>enable</Nullable>
        <Platforms>AnyCPU</Platforms>
        <NoWarn>$(NoWarn);1591</NoWarn>
        <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
        <AttachWeakETagToCompressedAssetsDuringDevelopment>true</AttachWeakETagToCompressedAssetsDuringDevelopment>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="10.0.0" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="10.0.0" PrivateAssets="all" />
        <PackageReference Include="Soenneker.DataTables.Extensions.ServerSideRequest" Version="4.0.49" />
        <PackageReference Include="Soenneker.Dtos.Results.Paged" Version="4.0.12" />
        <PackageReference Include="Soenneker.Extensions.Configuration.Logging" Version="4.0.70" />
        <PackageReference Include="Soenneker.Extensions.Serilog.LogEventLevels" Version="4.0.113" />
        <PackageReference Include="Soenneker.Serilog.Sinks.Browser.Blazor" Version="4.0.206" />
        <PackageReference Include="Soenneker.Utils.AutoBogus" Version="4.0.816" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\src\Soenneker.Quark.Suite.csproj" />
    </ItemGroup>
</Project>