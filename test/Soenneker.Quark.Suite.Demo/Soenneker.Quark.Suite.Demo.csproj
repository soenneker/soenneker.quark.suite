<?xml version="1.0" encoding="utf-8"?>

<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <Nullable>enable</Nullable>
        <Platforms>AnyCPU</Platforms>
        <NoWarn>$(NoWarn);1591</NoWarn>
        <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
        <AttachWeakETagToCompressedAssetsDuringDevelopment>true</AttachWeakETagToCompressedAssetsDuringDevelopment>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="10.0.3" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="10.0.3" PrivateAssets="all" />
        <PackageReference Include="Soenneker.DataTables.Extensions.ServerSideRequest" Version="4.0.77" />
        <PackageReference Include="Soenneker.Dtos.Results.Paged" Version="4.0.16" />
        <PackageReference Include="Soenneker.Extensions.Configuration.Logging" Version="4.0.105" />
        <PackageReference Include="Soenneker.Extensions.Serilog.LogEventLevels" Version="4.0.144" />
        <PackageReference Include="Soenneker.Quark.Gen.Lucide" Version="4.0.12">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="Soenneker.Serilog.Sinks.Browser.Blazor" Version="4.0.248" />
        <PackageReference Include="Soenneker.Utils.AutoBogus" Version="4.0.848" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\src\Soenneker.Quark.Suite.csproj" />
    </ItemGroup>
</Project>