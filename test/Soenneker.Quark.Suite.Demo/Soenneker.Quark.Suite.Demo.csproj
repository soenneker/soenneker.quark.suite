<?xml version="1.0" encoding="utf-8"?>

<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <Nullable>enable</Nullable>
        <Platforms>AnyCPU</Platforms>
        <NoWarn>$(NoWarn);1591</NoWarn>
        <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
        <AttachWeakETagToCompressedAssetsDuringDevelopment>true</AttachWeakETagToCompressedAssetsDuringDevelopment>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="10.0.0-rc.2.25502.107" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="10.0.0-rc.2.25502.107" PrivateAssets="all" />
        <PackageReference Include="Soenneker.DataTables.Extensions.ServerSideRequest" Version="3.0.36" />
        <PackageReference Include="Soenneker.Dtos.Results.Paged" Version="3.0.8" />
        <PackageReference Include="Soenneker.Extensions.Configuration.Logging" Version="3.0.61" />
        <PackageReference Include="Soenneker.Extensions.Serilog.LogEventLevels" Version="3.0.105" />
        <PackageReference Include="Soenneker.Serilog.Sinks.Browser.Blazor" Version="3.0.196" />
        <PackageReference Include="Soenneker.Utils.AutoBogus" Version="3.0.809" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\src\Soenneker.Quark.Suite.csproj" />
    </ItemGroup>
</Project>