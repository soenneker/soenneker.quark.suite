<?xml version="1.0" encoding="utf-8"?><Project Sdk="Microsoft.NET.Sdk.Razor">

    <PropertyGroup>
        <TargetFrameworks>net9.0</TargetFrameworks>
        <Nullable>enable</Nullable>
        <Platforms>AnyCPU</Platforms>
        <NoWarn>$(NoWarn);1591</NoWarn>
    </PropertyGroup>

    <PropertyGroup>
        <Description>The entire suite of Quark elements, all in one library.</Description>
        <Version Condition="'$(BUILD_VERSION)' == ''">3.0.0</Version>
        <Version Condition="'$(BUILD_VERSION)' != ''">$(BUILD_VERSION)</Version>
        <AssemblyName>Soenneker.Quark.Suite</AssemblyName>
        <PackageId>Soenneker.Quark.Suite</PackageId>
        <PackageTags>quark suite html elements blazor ui wrapper css quarksuite blazorlibrary csharp dotnet c# .net</PackageTags>
        <Copyright>Copyright © 2025 Jake Soenneker</Copyright>
        <Authors>Jake Soenneker</Authors>
        <Company>https://soenneker.com</Company>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <IsPackable>true</IsPackable>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/soenneker/soenneker.quark.suite</PackageProjectUrl>
        <RepositoryUrl>https://github.com/soenneker/soenneker.quark.suite</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <RepositoryBranch>main</RepositoryBranch>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <LangVersion>latest</LangVersion>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageIcon>icon.png</PackageIcon>
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild></PropertyGroup>
    
    <ItemGroup>
        <None Include="..\README.md" Pack="true" PackagePath="\" />
        <None Include="..\LICENSE" Pack="true" PackagePath="\" />
        <None Include="..\icon.png" Pack="true" PackagePath="\" />
    </ItemGroup>

    <ItemGroup>
        
        <!-- Base Quark Packages (includes most enums) -->
        <PackageReference Include="Soenneker.Quark.Components.Cancellable" Version="3.0.10" />
        
        <!-- Additional Enum Packages (not included in Components) -->
        
        <!-- All Quark Component Packages -->
        <PackageReference Include="Soenneker.Quark.Anchors" Version="3.0.4" />
        <PackageReference Include="Soenneker.Quark.Buttons" Version="3.0.6" />
        <PackageReference Include="Soenneker.Quark.Codes" Version="3.0.5" />
        <PackageReference Include="Soenneker.Quark.Columns" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.Containers" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.Divs" Version="3.0.3" />
        <PackageReference Include="Soenneker.Quark.Headings" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.Images" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.Labels" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.ListItems" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.OrderedLists" Version="3.0.4" />
        <PackageReference Include="Soenneker.Quark.Paragraphs" Version="3.0.2" />
        <PackageReference Include="Soenneker.Quark.Rows" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.Spans" Version="3.0.2" />
        <PackageReference Include="Soenneker.Quark.Strongs" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.Table" Version="3.0.54" />
        <PackageReference Include="Soenneker.Quark.Texts" Version="3.0.1" />
        <PackageReference Include="Soenneker.Quark.UnorderedLists" Version="3.0.1" />
    </ItemGroup>
</Project>
