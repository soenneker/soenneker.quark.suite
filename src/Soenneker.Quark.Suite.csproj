<?xml version="1.0" encoding="utf-8"?>

<Project Sdk="Microsoft.NET.Sdk.Razor">

    <PropertyGroup>
        <TargetFrameworks>net9.0</TargetFrameworks>
        <Nullable>enable</Nullable>
        <Platforms>AnyCPU</Platforms>
        <RootNamespace>Soenneker.Quark</RootNamespace>
        <GenerateDefaultNamespace>false</GenerateDefaultNamespace>
        <NoWarn>$(NoWarn);1591;IDE0130</NoWarn>
    </PropertyGroup>

    <PropertyGroup>
        <Description>The entire suite of Quark elements, all in one library.</Description>
        <Version Condition="'$(BUILD_VERSION)' == ''">3.0.0</Version>
        <Version Condition="'$(BUILD_VERSION)' != ''">$(BUILD_VERSION)</Version>
        <AssemblyName>Soenneker.Quark.Suite</AssemblyName>
        <PackageId>Soenneker.Quark.Suite</PackageId>
        <PackageTags>quark suite html elements blazor ui wrapper css quarksuite blazorlibrary csharp dotnet c# .net</PackageTags>
        <Copyright>Copyright © 2025 Jake Soenneker</Copyright>
        <Authors>Jake Soenneker</Authors>
        <Company>https://soenneker.com</Company>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <IsPackable>true</IsPackable>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/soenneker/soenneker.quark.suite</PackageProjectUrl>
        <RepositoryUrl>https://github.com/soenneker/soenneker.quark.suite</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <RepositoryBranch>main</RepositoryBranch>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <LangVersion>latest</LangVersion>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageIcon>icon.png</PackageIcon>
        <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\README.md" Pack="true" PackagePath="\" />
        <None Include="..\LICENSE" Pack="true" PackagePath="\" />
        <None Include="..\icon.png" Pack="true" PackagePath="\" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Soenneker.Quark.Alerts" Version="3.0.31" />
        <PackageReference Include="Soenneker.Quark.Badges" Version="3.0.27" />
        <PackageReference Include="Soenneker.Quark.Bars" Version="3.0.31" />
        <PackageReference Include="Soenneker.Quark.Breadcrumbs" Version="3.0.36" />
        <PackageReference Include="Soenneker.Quark.Cards" Version="3.0.46" />
        <PackageReference Include="Soenneker.Quark.Checks" Version="3.0.18" />
        <PackageReference Include="Soenneker.Quark.Codes" Version="3.0.27" />
        <PackageReference Include="Soenneker.Quark.Columns" Version="3.0.29" />
        <PackageReference Include="Soenneker.Quark.Components" Version="3.0.59" />
        <PackageReference Include="Soenneker.Quark.Containers" Version="3.0.22" />
        <PackageReference Include="Soenneker.Quark.DateEdits" Version="3.0.3" />
        <PackageReference Include="Soenneker.Quark.DatePickers" Version="3.0.17" />
        <PackageReference Include="Soenneker.Quark.Dropdowns" Version="3.0.2" />
        <PackageReference Include="Soenneker.Quark.Fields" Version="3.0.37" />
        <PackageReference Include="Soenneker.Quark.Icons" Version="3.0.13" />
        <PackageReference Include="Soenneker.Quark.Layouts" Version="3.0.13" />
        <PackageReference Include="Soenneker.Quark.ListGroups" Version="3.0.2" />
        <PackageReference Include="Soenneker.Quark.MemoEdits" Version="3.0.18" />
        <PackageReference Include="Soenneker.Quark.Modals" Version="3.0.41" />
        <PackageReference Include="Soenneker.Quark.NumericEdits" Version="3.0.18" />
        <PackageReference Include="Soenneker.Quark.Offcanvases" Version="3.0.31" />
        <PackageReference Include="Soenneker.Quark.Paginations" Version="3.0.30" />
        <PackageReference Include="Soenneker.Quark.Progresses" Version="3.0.19" />
        <PackageReference Include="Soenneker.Quark.Radios" Version="3.0.18" />
        <PackageReference Include="Soenneker.Quark.Rows" Version="3.0.27" />
        <PackageReference Include="Soenneker.Quark.Sections" Version="3.0.14" />
        <PackageReference Include="Soenneker.Quark.Sliders" Version="3.0.23" />
        <PackageReference Include="Soenneker.Quark.Smalls" Version="3.0.4" />
        <PackageReference Include="Soenneker.Quark.Snackbars" Version="3.0.30" />
        <PackageReference Include="Soenneker.Quark.Steps" Version="3.0.47" />
        <PackageReference Include="Soenneker.Quark.Strongs" Version="3.0.28" />
        <PackageReference Include="Soenneker.Quark.Switches" Version="3.0.30" />
        <PackageReference Include="Soenneker.Quark.Table" Version="3.0.105" />
        <PackageReference Include="Soenneker.Quark.Tabs" Version="3.0.32" />
        <PackageReference Include="Soenneker.Quark.TextEdits" Version="3.0.17" />
        <PackageReference Include="Soenneker.Quark.Texts" Version="3.0.27" />
    </ItemGroup>
</Project>