        <PackageReference Include="Soenneker.Quark.Enums.Flexes" Version="4.0.14" />
        <PackageReference Include="Soenneker.Quark.Enums.Floats" Version="4.0.16" />
        <PackageReference Include="Soenneker.Quark.Enums.FontStyles" Version="4.0.12" />
        <PackageReference Include="Soenneker.Quark.Enums.FontWeights" Version="4.0.13" />
        <PackageReference Include="Soenneker.Quark.Enums.GlobalKeywords" Version="4.0.7" />
