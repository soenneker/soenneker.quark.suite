@namespace Soenneker.Quark
@inherits Soenneker.Quark.Element
@implements IArticle

<article @attributes="BuildAttributes()">
	@ChildContent
</article>

@code {
    protected override Dictionary<string, object> BuildAttributes()
    {
        var attributes = base.BuildAttributes();
        BuildClassAttribute(attributes, (ref cls) => AppendClass(ref cls, "q-article"));
        return attributes;
    }
}

