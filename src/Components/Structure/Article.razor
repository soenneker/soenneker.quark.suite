@namespace Soenneker.Quark
@inherits Soenneker.Quark.Element
@implements IArticle

<article @attributes="BuildAttributes()">
	@ChildContent
</article>

@code {

    protected override void BuildAttributesCore(Dictionary<string, object> attributes)
    {
        BuildClassAttribute(attributes, (ref cls) => AppendClass(ref cls, "q-article"));
    }
}

