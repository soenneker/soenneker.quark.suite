@namespace Soenneker.Quark
@inherits Soenneker.Quark.Element
@implements IFigure

<figure @attributes="BuildAttributes()">
	@ChildContent
</figure>

@code {
    protected override void BuildAttributesCore(Dictionary<string, object> attributes)
    {
        BuildClassAttribute(attributes, (ref cls) => AppendClass(ref cls, "q-figure"));
    }
}

