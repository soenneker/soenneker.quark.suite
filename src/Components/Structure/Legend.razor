@namespace Soenneker.Quark
@inherits Soenneker.Quark.Element
@implements ILegend

<legend @attributes="BuildAttributes()">
	@ChildContent
</legend>

@code {
    protected override Dictionary<string, object> BuildAttributes()
    {
        var attributes = base.BuildAttributes();
        BuildClassAttribute(attributes, (ref cls) => AppendClass(ref cls, "q-legend"));
        return attributes;
    }
}

